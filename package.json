{
  "name": "google-books",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eliuth4k9/g-book.git"
  },
  "author": "Steve Hernandez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Eliuth4k9/g-book/issues"
  },
  "homepage": "https://github.com/Eliuth4k9/g-book#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.4",
    "nodemon": "^2.0.7",
    "tsparticles": "^1.26.3"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "webpack-assets-manifest": "^5.0.5"
  },
  "description": ""
}
